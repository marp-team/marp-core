{
  "name": "@marp-team/marp-core",
  "version": "4.2.0",
  "description": "The core of Marp tools",
  "license": "MIT",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "keywords": [
    "marp",
    "markdown",
    "parser",
    "slide",
    "deck",
    "presentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marp-team/marp-core"
  },
  "main": "lib/marp.js",
  "types": "types/src/marp.d.ts",
  "files": [
    "lib/",
    "types/",
    "browser.js",
    "browser.d.ts"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "npm -s run clean && rollup -c",
    "check:audit": "npm audit",
    "check:format": "npm -s run format -- -c",
    "check:ts": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier \"**/*.{css,js,json,md,mjs,scss,ts,yaml,yml}\"",
    "format:write": "npm -s run format -- --write",
    "lint:js": "eslint --cache",
    "lint:css": "stylelint \"{src,themes}/**/*.{css,scss}\"",
    "prepack": "npm-run-all --parallel check:* lint:* test:coverage --parallel build types",
    "preversion": "run-p check:* lint:* test:coverage",
    "sandbox": "run-p watch sandbox:watch",
    "sandbox:watch": "mkdirp lib && nodemon",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types",
    "version": "curl https://raw.githubusercontent.com/marp-team/actions/v1/lib/scripts/version.js | node && git add -A CHANGELOG.md",
    "watch": "rollup -w -c"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "nodemonConfig": {
    "watch": "./lib",
    "exec": "npx @marp-team/marp-cli@latest -y"
  },
  "devDependencies": {
    "@csstools/postcss-minify": "^2.0.5",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@tsconfig/node18": "^18.2.6",
    "@tsconfig/recommended": "^1.0.13",
    "@twemoji/api": "^17.0.2",
    "@types/jest": "^30.0.0",
    "autoprefixer": "^10.4.27",
    "cheerio": "^1.2.0",
    "cssnano": "^7.1.2",
    "emoji-regex": "10.6.0",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.15.0",
    "github-markdown-css": "5.9.0",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jest-plugin-context": "^2.9.0",
    "markdown-it": "^14.1.1",
    "markdown-it-emoji": "^3.0.0",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.14",
    "npm-check-updates": "^19.6.3",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "postcss-url": "^10.1.3",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "rollup": "^4.59.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-string": "^3.0.0",
    "sass": "^1.97.3",
    "self-closing-tags": "^1.0.1",
    "stylelint": "^17.4.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "ts-jest": "29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1"
  },
  "dependencies": {
    "@marp-team/marpit": "^3.2.1",
    "@marp-team/marpit-svg-polyfill": "^2.1.0",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.33",
    "mathjax-full": "^3.2.2",
    "postcss-selector-parser": "^7.1.1",
    "xss": "^1.0.15"
  },
  "overrides": {
    "@rollup/plugin-terser": {
      "serialize-javascript": "^7.0.3"
    },
    "postcss-url": {
      "minimatch": "^3.1.5"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
